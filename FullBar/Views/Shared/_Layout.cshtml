@{
    FullBar.Repositories.UsuarioRepository ur = new FullBar.Repositories.UsuarioRepository();
    var model = ur.ListaAlunos();
}

<body>
    
    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>




<!DOCTYPE html>
<html>
<div id="divconteudototal">
<meta http-equiv="content-type" content="text/html;charset=utf-8" />
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Favicon-->
    <link rel="icon" href="~/Content/themes/base_1/images/favicon.ico" type="image/x-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,700&amp;subset=latin,cyrillic-ext" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core Css -->
    <link href="~/Content/themes/base_1/plugins/bootstrap/css/bootstrap.css" rel="stylesheet">
    <!-- Waves Effect Css -->
    <link href="~/Content/themes/base_1/plugins/node-waves/waves.css" rel="stylesheet" />
    <!-- Animation Css -->
    <link href="~/Content/themes/base_1/plugins/animate-css/animate.css" rel="stylesheet" />
    <!-- Colorpicker Css -->
    <link href="~/Content/themes/base_1/plugins/bootstrap-colorpicker/css/bootstrap-colorpicker.css" rel="stylesheet" />
    <!-- Dropzone Css -->
    <link href="~/Content/themes/base_1/plugins/dropzone/dropzone.css" rel="stylesheet">
    <!-- Multi Select Css -->
    <link href=".~/Content/themes/base_1/plugins/multi-select/css/multi-select.css" rel="stylesheet">
    <!-- Bootstrap Spinner Css -->
    <link href="~/Content/themes/base_1/plugins/jquery-spinner/css/bootstrap-spinner.css" rel="stylesheet">
    <!-- Bootstrap Tagsinput Css -->
    <link href="~/Content/themes/base_1/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet">
    <!-- Bootstrap Select Css -->
    <link href="~/Content/themes/base_1/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
    <!-- noUISlider Css -->
    <link href="~/Content/themes/base_1/plugins/nouislider/nouislider.min.css" rel="stylesheet" />
    <!-- Custom Css -->
    <link href="~/Content/themes/base_1/css/style.css" rel="stylesheet">
    <!-- AdminBSB Themes. You can choose a theme from css/themes instead of get all themes -->
    <link href="~/Content/themes/base_1/css/themes/all-themes.css" rel="stylesheet" />
</head>
<body class="theme-grey">
    <!-- Page Loader -->
    <div class="page-loader-wrapper">
        <div class="loader">
            <div class="preloader">
                <div class="spinner-layer pl-red">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
            <p>Aguarde...</p>
        </div>
    </div>
    <!-- #END# Page Loader -->
    <!-- Overlay For Sidebars -->
    <div class="overlay"></div>
    <!-- #END# Overlay For Sidebars -->
  

    <nav class="navbar">
        <div class="container-fluid">
            <div class="navbar-header">
                <a href="javascript:void(0);" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse" aria-expanded="false"></a>
                <a href="javascript:void(0);" class="bars"></a>
                <a class="navbar-brand" href="@Url.Action("Index","Usuario")"><img style="margin-top:-17px;width:71px" src="~/Content/themes/base_1/images/logo.JPG" /></a>
            </div>
            <div class="collapse navbar-collapse" id="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle" data-toggle="dropdown" role="button">
                            <i class="material-icons">notifications</i>
                            <span class="label-count">@{
                                string valores = model.Count().ToString();
                            } @valores</span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="header">NOTIFICAÇÕES</li>
                            <li class="body">
                                <ul class="menu">
                                    <li>
                                        

                                       
                                            @foreach (var item in model)
                                            {
                                                <a href="javascript:void(0);">
                                                    <div class="icon-circle bg-light-green">
                                                        <i class="material-icons">person_add</i>
                                                    </div>
                                                    <div class="menu-info">
                                                        <h4>Você adicionou um novo aluno</h4>
                                                        <p>
                                                            <i class="material-icons">access_time</i> @item.Nome 
                                                        </p>
                                                    </div>
                                                </a>
                                             }
                                        

                                    </li>
                                   
                                  
                                </ul>
                            </li>
                            
                        </ul>
                    </li>
                    <!-- #END# Notifications -->
                   
                    <li class="pull-right"><a href="javascript:void(0);" class="js-right-sidebar" data-close="true"><i class="material-icons">more_vert</i></a></li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- #Top Bar -->
    <section>
        <!-- Left Sidebar -->
        <aside id="leftsidebar" class="sidebar">
            <!-- User Info -->
            <div class="user-info">
                <div class="image">
                    <img src="~/Content/themes/base_1/images/usuario.jpg" width="48" height="48" alt="User" />
                    
                </div>
                <div class="info-container">
                    <div class="name" data-toggle="dropdown" style="color:black;" aria-haspopup="true" aria-expanded="false">
                        Leonardo Gomes
                    </div>
                    <div class="email" style="color:black;">
                        leonardogomes17@gmail.com
                    </div>
                    <div class="btn-group user-helper-dropdown">
                        <i class="material-icons" data-toggle="dropdown" style="color:black;" aria-haspopup="true" aria-expanded="true">keyboard_arrow_down</i>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="#"><i class="material-icons">person</i>Meus Dados</a></li>
                            <li role="seperator" class="divider"></li>
                            <li><a href="#"><i class="material-icons">input</i>Sair</a></li>
                        </ul>
                    </div>
                </div>
            </div>
            <!-- #User Info -->
            <!-- Menu -->
            <div class="menu">
                <ul class="list">
                    <li class="header">MENU</li>
                    <li>
                        <a href="@Url.Action("Index", "Usuario")">
                            <i class="material-icons">home</i>
                            <span>Home</span>
                        </a>
                    </li>


                    <li class="active" id="ocultotesting" style="display:none;">
                       
                    </li>




                    <li>
                        <a href="javascript:void(0);" class="menu-toggle">
                            <i class="material-icons">person</i>
                            <span>Usuario</span>
                        </a>
                        <ul class="ml-menu">
                            
                            <li>
                                <a href="javascript:void(0);" class="menu-toggle">
                                    <span>Cadastrar</span>
                                </a>
                                <ul class="ml-menu">
                                   
                                    <li>
                                        <a href="@Url.Action("CadastroUsuario", "Usuario")">
                                            <span>Cadastrar Usuarios</span>
                                        </a>
                                       
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="javascript:void(0);" class="menu-toggle">
                                    <span>Alterar</span>
                                </a>
                                <ul class="ml-menu">
                                    
                                    <li>
                                        <a href="@Url.Action("AlterarUsuario", "Usuario")">
                                            <span>Alterar Usuarios</span>
                                        </a>
                                        
                                    </li>
                                </ul>
                            </li>
                        </ul>
                    </li>
                    
                    @*//CONFIGURAÇÕES*@

                    


                </ul>
            </div>
            <!-- #Menu -->
            <!-- Footer -->
            <div class="legal">
                <div class="copyright">
                    &copy; 2018 <a target="_blank" href="http://fullbar.com.br">FullBar</a>.
                </div>
                <div class="version">
                    <b>Version: </b> 0.0.0
                </div>
            </div>
            <!-- #Footer -->
        </aside>
        <!-- #END# Left Sidebar -->
        <!-- Right Sidebar -->
        <aside id="rightsidebar" class="right-sidebar">
            <ul class="nav nav-tabs tab-nav-right" role="tablist">
                <li role="presentation" class="active"><a href="#skins" data-toggle="tab">CORES LAYOUT</a></li>
                <li role="presentation"><a href="#settings" data-toggle="tab">CONFIGURAÇÕES</a></li>
            </ul>
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane fade in active in active" id="skins">
                    <ul class="demo-choose-skin">
                      
                        <li data-theme="grey" class="active">
                            <div class="grey"></div>
                            <span>Cinza</span>
                        </li>
                        
                    </ul>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="settings">
                    <div class="demo-settings">
                        <p>Configurações Gerais</p>
                        
                        <p>SISTEMA CONFIGURAÇÕES</p>
                        <ul class="setting-list">
                            <li>
                                <span>Notificações</span>
                                <div class="switch">
                                    <label><input type="checkbox" checked><span class="lever"></span></label>
                                </div>
                            </li>

                        </ul>
                        
                    </div>
                </div>
            </div>
        </aside>
        <!-- #END# Right Sidebar -->
    </section>
    <section class="content">
        <div class="container-fluid">
            @*<form id="form1" runat="server">*@
                @RenderBody()
            @*</form>*@
        </div>
    </section>
    <!-- Jquery Core Js -->
    <script src="~/Content/themes/base_1/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap Core Js -->
    <script src="~/Content/themes/base_1/plugins/bootstrap/js/bootstrap.js"></script>
    <!-- Select Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/bootstrap-select/js/bootstrap-select.js"></script>
    <!-- Slimscroll Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/jquery-slimscroll/jquery.slimscroll.js"></script>
    <!-- Bootstrap Colorpicker Js -->
    <script src="~/Content/themes/base_1/plugins/bootstrap-colorpicker/js/bootstrap-colorpicker.js"></script>
    <!-- Dropzone Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/dropzone/dropzone.js"></script>
    <!-- Input Mask Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/jquery-inputmask/jquery.inputmask.bundle.js"></script>
    <!-- Multi Select Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/multi-select/js/jquery.multi-select.js"></script>
    <!-- Jquery Spinner Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/jquery-spinner/js/jquery.spinner.js"></script>
    <!-- Bootstrap Tags Input Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/bootstrap-tagsinput/bootstrap-tagsinput.js"></script>
    <!-- noUISlider Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/nouislider/nouislider.js"></script>
    <!-- Waves Effect Plugin Js -->
    <script src="~/Content/themes/base_1/plugins/node-waves/waves.js"></script>
    <!-- Custom Js -->
    <script src="~/Content/themes/base_1/js/admin.js"></script>
    <script src="~/Content/themes/base_1/js/pages/forms/advanced-form-elements.js"></script>
    <!-- Demo Js -->
    <script src="~/Content/themes/base_1/js/demo.js"></script>
</body>

</div>
</html>





